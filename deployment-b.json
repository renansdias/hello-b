{
	"apiVersion": "extensions/v1beta1",
	"kind": "Deployment",
	"metadata": {
		"name": "deployment-b"
	},
	"spec": {
		"replicas": 1,
		"template": {
			"metadata": {
				"labels": {
					"app": "hello-b"
				}
			},
			"spec": {
				"containers": [
					{
						"name": "hello-b",
						"image": "renansdias/hello-b:1.0.0",
						"ports": [
							{
								"containerPort": 2345,
								"hostPort": 2345,
								"name": "hellob"
							}
						]
					}
				]
			}
		}
	}
}